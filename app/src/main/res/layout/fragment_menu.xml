<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragmentos.MenuFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_margin="10dp">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_margin="5dp"
            android:gravity="center">
            <ImageButton
                android:id="@+id/ibCartas"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/crt48"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:background="#0000"/>
            <ImageButton
                android:id="@+id/ibCarrito"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/car48"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:background="#0000"/>
            <ImageButton
                android:id="@+id/ibUbicanos"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/ubi48"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:background="#0000"/>
            <ImageButton
                android:id="@+id/ibConfiguracion"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/config48"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:background="#0000"/>
        </LinearLayout>
    </LinearLayout>

</FrameLayout>